{
    "name": "php-etl/magento2-api-client",
    "description": "This package provides Jane-PHP generated API models and client based on the OpenAPI specification for Magento 2.3.x",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kiboko\\Magento\\": ["src/","generated/"]
        }
    },
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "jane-php/open-api-runtime": "^7.3",
        "symfony/serializer": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15",
        "jane-php/open-api-2": "^7.5"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
