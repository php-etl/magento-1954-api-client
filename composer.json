{
    "name": "php-etl/magento2-api-client",
    "description": "This package provides Jane-PHP generated API models and client based on the OpenAPI specification for Magento 2.3.x",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kiboko\\Magento\\V2_1\\": "src/v2_1",
            "Kiboko\\Magento\\V2_2\\": "src/v2_2",
            "Kiboko\\Magento\\V2_3\\": "src/v2_3",
            "Kiboko\\Magento\\V2_4\\": "src/v2_4"
        }
    },
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "jane-php/open-api-runtime": "^7.3",
        "symfony/serializer": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "jane-php/open-api-3": "^7.3"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
